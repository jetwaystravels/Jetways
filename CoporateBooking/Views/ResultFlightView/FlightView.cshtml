        // Bussines Class Validation radio button
        function toggleRadio(event, radioId) {
            event.preventDefault();
            document.querySelectorAll('.form-radio-input').forEach(radio => {
                radio.checked = false;
            });
            var radioButton = document.getElementById(radioId);
            radioButton.checked = true;
        }


        //*********End***************/

        // Sorting - Filter Start
        var sortOrderName;
        const sortLinks = document.querySelectorAll('.sort-link');
        const sortOrders = {
            name: 'desc',
            price: 'desc',
            arrival: 'desc',
            duration: 'desc',
            deprt: 'desc'
        };

        // Add click event listener to each sort link
        sortLinks.forEach(sortLink => {

            sortLink.addEventListener('click', function (e) {
                e.preventDefault();
                const sortType = this.dataset.sort.split('_')[0];
                const currentSortOrder = sortOrders[sortType];
                sortOrders[sortType] = currentSortOrder === 'desc' ? 'asc' : 'desc';
                const newSortOrder = currentSortOrder === 'desc' ? `${sortType}_desc` : `${sortType}_asc`;
                this.dataset.sort = newSortOrder;
                performSorting(newSortOrder);

                // Make AJAX request to post the sorting order to the controller
                postSortOrder(newSortOrder);
            });
        });

        function performSorting(sortOrder) {
            sortOrderName = sortOrder;
            getFiltered();
        }



        var selectedDepartureTimes = [];
        var selectedArrivalTimes = [];

        $('.flt-Tm-slt').click(function () {
            let departureTime = $(this).data('departure');
            let arrivalTime = $(this).data('arrival');

            $(this).toggleClass('active');

            if ($(this).hasClass('active')) {
                // Add selected times to arrays
                if (!selectedDepartureTimes.includes(departureTime)) {
                    selectedDepartureTimes.push(departureTime);
                }
                if (!selectedArrivalTimes.includes(arrivalTime)) {
                    selectedArrivalTimes.push(arrivalTime);
                }
            } else {
                // Remove deselected times from arrays
                selectedDepartureTimes = selectedDepartureTimes.filter(function (item) {
                    return item !== departureTime;
                });
                selectedArrivalTimes = selectedArrivalTimes.filter(function (item) {
                    return item !== arrivalTime;
