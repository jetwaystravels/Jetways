            document.querySelector('.decrement2.sub.btn_sub').addEventListener('click', function () {
                decrementValue(infantInputField, infantCountElement);
            });

            // Function to increment the input value and update the count element
            function incrementValue(inputField, countElement) {
                const currentValue = parseInt(inputField.value);
                if (currentValue < parseInt(inputField.max)) {
                    inputField.value = currentValue + 1;
                    countElement.textContent = currentValue + 1;
                }
            }

            // Function to decrement the input value and update the count element
            function decrementValue(inputField, countElement) {
                const currentValue = parseInt(inputField.value);
                if (currentValue > parseInt(inputField.min)) {
                    inputField.value = currentValue - 1;
                    countElement.textContent = currentValue - 1;
                }
            }
        });
