    <script>

        //***********Strip Validation*/
        $(document).ready(function () {
           

            var maxField = 6;
            var maxField_adult = 9;
            var count_adult = parseInt(localStorage.getItem("adultcount")) || 1; 
            var count_child = 0;
            var count_infant = 0;

            $('#count_adult').text(count_adult);
            $('#field_adult').val(count_adult);

            $('.increment').on('click', function () {
                if (count_adult < maxField_adult) {
                    count_adult++;
                    $('#count_adult').text(count_adult);
                    $('#field_adult').val(count_adult);
                    localStorage.setItem("adultcount", count_adult);
                }
            });

            $('.decrement').on('click', function () {
                if (count_adult > 1) {
                    count_adult--;
                    $('#count_adult').text(count_adult);
                    $('#field_adult').val(count_adult);
                    localStorage.setItem("adultcount", count_adult); // Store adult count in local storage

                    // Adjust infant count to match adult count
                    if (count_infant > count_adult) {
                        count_infant = count_adult;
                        $('#count_infant').text(count_infant);
                        $('#field_infant').val(count_infant);
                    }
                }
            });

            $('.increment1').on('click', function () {
                if (count_child < maxField) {
                    count_child++;
                    $('#count_child').text(count_child);
                    $('#field_child').val(count_child);
                }
            });

            $('.decrement1').on('click', function () {
                if (count_child > 0) {
                    count_child--;
                    $('#count_child').text(count_child);
                    $('#field_child').val(count_child);
                }
            });

            $('.increment2').on('click', function (event) {
                debugger;
                if (count_infant < maxField) {
                    var adultcounttotal = parseInt(localStorage.getItem("adultcount")) || 1; 
                    if (count_infant >= adultcounttotal) {
                        alert("Number of infants cannot be more than adults");
                        return false;
                        event.preventDefault();
                    }
                        count_infant++;
                        $('#count_infant').text(count_infant);
                        $('#field_infant').val(count_infant);
                   
                }
            });

            $('.decrement2').on('click', function () {
                if (count_infant > 0) {
                    count_infant--;
                    $('#count_infant').text(count_infant);
                    $('#field_infant').val(count_infant);
                }
            });
            

        });



        // TextBox Validation
        function validateInputs(event) {
            const origin = document.getElementById('myInput').value;
            const destination = document.getElementById('myInput1').value;
            const beginDate = document.getElementById('Oneway-date').value;
            const endDateInput = document.getElementById('Oneway-endDate');
            const endDate = endDateInput.value;
            if (origin == destination) {
                alert("Departure and Arrival Airport are same please change it..")
                return false;
                event.preventDefault(); // Stop the form submission

            }

            if (!origin) {
                alert('Please enter the origin.');
                return false;
            }
            if (!destination) {
                alert('Please enter the destination.');
                return false;
            }
            if (!beginDate) {
                alert('Please select a start date.');
                return false;
            }
            if (!endDateInput.disabled && !endDate) {
                alert('Please select an end date.');
                return false;
            }

            var adultCount = parseInt(document.getElementById('field_adult').value);
            var childCount = parseInt(document.getElementById('field_child').value);
            //var infantCount = parseInt(document.getElementById('field_infant').value);
            var totalCount = adultCount + childCount;
            if (totalCount > 9) {
                alert("The combined total of adults and children must be 9 or less.");
                return false;
                event.preventDefault();
            }
           
            var adulttravelcount = $('#field_adult').val();
            var infanttravelcount = $('#field_infant').val();
            if (infanttravelcount > adulttravelcount || infanttravelcount > 6) {
                alert("Number of infants cannot be more than adults or exceed 6.");
                return false;
                event.preventDefault();

            }
            return true;
        }



        // Validation Oneway , Roundtrip, multicity
        function checkRadio(radio) {
            const endDateInput = document.getElementById('Oneway-endDate');
            const beginDateInput = document.getElementById('Oneway-date');
            if (radio.value === 'OneWay') {
                endDateInput.disabled = true;
                endDateInput.value = '';
                endDateInput.style.backgroundColor = '#0e4d74';
            }
            else if (radio.value === 'RoundTrip') {
                endDateInput.disabled = false;
                endDateInput.value = beginDateInput.value;
                endDateInput.style.backgroundColor = '';
                endDateInput.style.backgroundColor = '#0E4D74';
            }
            else if (radio.value === 'AirlineRT') {
                endDateInput.disabled = false;
                endDateInput.value = beginDateInput.value;
                endDateInput.style.backgroundColor = '';
                endDateInput.style.backgroundColor = '#0e4d74';
            }
        }

        //Date Picker Validation
        $(function () {
            $("#Oneway-date").datepicker({
                dateFormat: 'yy-mm-dd',
                numberOfMonths: 2,
                maxDate: '+2m',
                minDate: '0',
                onSelect: function (selectedDate) {
                    var startDate = $(this).datepicker('getDate');
                    $("#Oneway-endDate").datepicker("option", "minDate", startDate);
                }
            });

            $("#Oneway-endDate").datepicker({
                dateFormat: 'yy-mm-dd',
                numberOfMonths: 2,
                maxDate: '+3m',
                minDate: '0',
                onSelect: function (selectedDate) {
                    var endDate = $(this).datepicker('getDate');
                    $("#Oneway-date").datepicker("option", "maxDate", endDate);
