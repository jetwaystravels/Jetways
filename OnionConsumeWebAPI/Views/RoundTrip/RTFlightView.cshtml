                    </div>
                    <nav id="mobile-nav">
                        <ul class="navbar-nav" id="navbar-nav">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#">
                                    Home
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link scroll" href="features">Features</a>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#">
                                    Pages
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link scroll" href="#">Screenshots</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link scroll" href="#">Pricing</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link scroll" href="#">Contact</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </header>
        <!-- ***** Header End ***** -->
        <!-- ***** Start inner search ***** -->
        <div class="inner-search">
            <!-- ***** Flight Search Area Start ***** -->
            <section class="section search-area bg-overlay  d-flex align-items-center">
                <div class="container">
                    <div class="row ">
                        <div class="col-12 col-md-7 col-lg-12">
                            <div class="row justify-content-center">
                                <div class="search-box">
                                    <div class="S-option">
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="Radio" value="option1">
                                            <label class="form-check-label">One way</label>
                                        </div>
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="Radio" value="option2" checked>
                                            <label class="form-check-label">Round Trip</label>
                                        </div>
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="Radio" value="option2">
                                            <label class="form-check-label">Multi City</label>
                                        </div>

                                    </div>
                                    <form asp-action="SearchResultFlight" asp-controller="FlightSearchIndex" method="post" id="multipleValuesForm">
                                        <div class="col-lg-12">
                                            <div class="row">
                                                <div class="col-12 col-md-10 col-lg-2 space autocomplete">
                                                    <label>Flying From</label>
                                                    <span class="input-group-text">
                                                        <img src="~/img/location-icon.png" alt="icon">
                                                        <input id="myInput" name="origin" type="" value="Delhi-DEL" class="form-control fisrt">
                                                    </span>
                                                </div>

                                                <div class="rplc-btn">
                                                    <img src="~/img/direction-arrows.png" />
                                                </div>

                                                <div class="col-12 col-md-10 col-lg-2 space autocomplete">
                                                    <label>Flying To</label>
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <img src="~/img/location-icon.png" alt="icon">
                                                        <input id="myInput1" name="destination" type="" value="Mumbai-BOM" class="form-control fisrt">
                                                    </span>
                                                </div>
                                                <div class="col-12 col-md-10 col-lg-2 space">
                                                    <label>Departure</label>
                                                    <span class="input-group-text">
                                                        <img src="~/img/calender-icon.png" alt="icon" style="background-color:#fff">
                                                        <input id="Rway-date" name="beginDate" type="text" placeholder="Date" class="form-control">
                                                    </span>
                                                </div>
                                                <div class="col-12 col-md-10 col-lg-2 space">
                                                    <label>Return</label>
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <img src="~/img/calender-icon.png" alt="icon" style="background-color:#fff">
                                                        <input id="Rway-endDate" name="endDate" type="text" placeholder="Date" class="form-control">
                                                    </span>
                                                </div>
                                                <div class="col-12 col-md-10 col-lg-2 space travellers">
                                                    <label>Travellers </label>
                                                    <div class="searchMenu-guests js-form-dd js-form-counters">
                                                        <div data-x-dd-click="searchMenu-guests">

                                                            <div class="text-15 text-light-1 ls-2 lh-16">
                                                                <span class="js-count-adult" id="count_adult">1</span> adults
                                                                -
                                                                <span class="js-count-child" id="count_child">0</span> childeren
                                                                -
                                                                <span class="js-count-infents" id="count_infant">0</span> Infants
                                                            </div>
                                                        </div>

                                                        <div class="searchMenu-guests__field shadow-2" data-x-dd="searchMenu-guests" data-x-dd-toggle="-is-active">

                                                            <div class="bg-white px-30 py-30 rounded-4">

                                                                @*  ------------------------ *@
                                                                <div class="d-flex y-gap-10 justify-between items-center">
                                                                    <div class="btn-incr-adult">
                                                                        <div class="text-15 fw-500">Adults</div>

                                                                        <input type="hidden" value="ADT" asp-for="@Model.MyProperty.passengercount.adulttype" />
                                                                        <div class="text-15 fw-500">Aged 12+ yrs</div>
                                                                    </div>
                                                                    <div class="btn-increase">
                                                                        <div class="decrement sub btn_sub">-</div>

                                                                        <input type="number" id="field_adult" class="input-field" value="1" asp-for="@Model.MyProperty.passengercount.adultcount" min="1" max="9">
                                                                        <div class="increment add btn_sub">+</div>
                                                                    </div>
                                                                </div>
                                                                <div class="border-top-light mt-24 mb-24"></div>
                                                                <div class="d-flex y-gap-10 justify-between items-center">
                                                                    <div class="btn-incr-chld">
                                                                        <div class="text-15 fw-500">
                                                                            Children
                                                                        </div>
                                                                        <input type="hidden" value="CHD" asp-for="@Model.MyProperty.passengercount.childtype" />
                                                                        <div class="text-15 fw-500">
                                                                            Aged 2-12 yrs
                                                                        </div>
                                                                    </div>
                                                                    <div class="btn-increase">
                                                                        <div class="decrement1 sub btn_sub">-</div>
                                                                        <input type="number" id="field_child" class="input-field" value="0" asp-for="@Model.MyProperty.passengercount.childcount" min="0" max="6">
                                                                        <div class="increment1 add btn_sub">+</div>

                                                                    </div>
                                                                </div>
                                                                <div class="border-top-light mt-24 mb-24"></div>
                                                                <div class="d-flex y-gap-10 justify-between items-center">
                                                                    <div class="infant-area">
                                                                        <div class="text-15 fw-500">Infants</div>
                                                                        <input type="hidden" value="INFT" asp-for="@Model.MyProperty.passengercount.infanttype" />
                                                                        <div class="text-15 fw-500">
                                                                            Below 2 yrs
                                                                        </div>
                                                                    </div>
                                                                    <div class="btn-increase">
                                                                        <div class="decrement2 sub btn_sub">-</div>
                                                                        <input type="number" id="field_infant" class="input-field" value="0" asp-for="@Model.MyProperty.passengercount.infantcount" min="0" max="6">
                                                                        <div class="increment2 add btn_sub">+</div>

                                                                    </div>
                                                                </div>

                                                                <div class="border-top-light mt-24 mb-24"></div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-10 col-lg-2"><button type="submit" class="btn btn-lg btn-block"><i class="fa fa-search mr-10" aria-hidden="true"></i>Modify Search</button></div>
                                                <div class="col-12 col-md-12 col-lg-12 search-bottom"></div>
                                                <div class="col-12 col-md-10 col-lg-9 pl-0">
                                                    <div class="form-group pt-2">
                                                        <div class="dropdown js-dropdown js-economy-active" style="display:inline-block;">
                                                            <div class="dropdown__button d-flex items-center  text-15" data-el-toggle=".js-economy-toggle" data-el-toggle-active=".js-economy-active">
                                                                <span class="js-dropdown-title">Show Class </span>
                                                                <i class="fa fa-angle-down ml-1 icon icon-chevron-sm-down" aria-hidden="true"></i>
                                                            </div>
                                                            <div class="toggle-element -dropdown  js-click-dropdown js-economy-toggle">
                                                                <div class="text-14 y-gap-8 js-dropdown-list">

                                                                    <div><a href="#" class="d-block js-dropdown-link">Economic</a></div>
                                                                    <div><a href="#" class="d-block js-dropdown-link">Premium</a></div>
                                                                    <div><a href="#" class="d-block js-dropdown-link">Business</a></div>
                                                                    <div><a href="#" class="d-block js-dropdown-link">First</a></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="custom-control custom-checkbox custom-control-inline">
                                                            <input type="checkbox" class="custom-control-input" id="defaultInline3">
                                                            <label class="custom-control-label" for="defaultInline3">Direct Flights</label>
                                                        </div>
                                                        <div class="custom-control custom-checkbox custom-control-inline">
                                                            <input type="checkbox" class="custom-control-input" id="defaultInline4">
                                                            <label class="custom-control-label" for="defaultInline4">Defence Fare</label>
                                                        </div>
                                                        <div class="custom-control custom-checkbox custom-control-inline">
                                                            <input type="checkbox" class="custom-control-input" id="defaultInline5">
                                                            <label class="custom-control-label" for="defaultInline5">Student Fare </label>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- ***** Flight Search Area End ***** -->

        </div>
        <!-- ***** End inner search ***** -->

        <section class="layout-pt-md layout-pb-md bg-light-2 search-result mb-5">
            <div class="container">
                <div class="row  y-gap-30">
                    <div class="col-xl-3 col-lg-4">
                        <aside class="sidebar py-20 px-20 rounded-4 bg-white">
                            <partial name="_RTFlightViewPartialView.cshtml" />
                        </aside>
                    </div>
                    <div class="col-xl-9 col-lg-8 ps-0">
                        <div class="row ">
                            <div class="col-xl-6 col-lg-6">

                                <div class="row y-gap-10 justify-between items-center">
                                    <div class="col-auto">
                                        <div class="text-18"><span class="fw-500">Showing - @ViewData["count"] results.</span> </div>
                                    </div>
                                    <div class="col-auto">
                                        <button class="button -blue-1 h-40 px-30 rounded-100 bg-blue-1-05 text-15 text-blue-1">
                                            <i class="fa fa-filter" aria-hidden="true"></i> Sort
                                        </button>
                                    </div>
                                </div>

                                <div class="row round1">
                                    <div class="col-12">
                                        <h6>@ViewData["origin"]<i class="fa fa-angle-double-right" aria-hidden="true"></i>@ViewData["destination"]</h6>
                                    </div>
                                </div>
                                <!----->
                                <form id="reconnectingFlight">
                                    @for (var j = 0; j < Model.SimpleAvailibilityaAddResponcelist.Count; j++)
                                    {
                                        <div class="py-10 px-20 bg-white mt-20">
                                            <div class="d-flex y-gap-30 justify-between">
                                                <div class="fare-round">
                                                    @if (Model.SimpleAvailibilityaAddResponcelist[j].Airline.ToString().ToLower().Equals("spicejet"))
                                                    {

                                                        <img class="size-40" src="~/img/flight-logo-icon/SG.png" alt="image">
                                                    }
                                                    else
                                                    {
                                                        <img class="size-40" src="~/img/flight-logo-icon/1.webp" alt="image">
                                                    }
                                                    @* <img class="size-40" src="~/img/flight-logo-icon/1.webp" alt="image"> *@
                                                    <input type="hidden" value="@Model.SimpleAvailibilityaAddResponcelist[j].uniqueId" />
                                                    @*<input type="hidden" class="carrierCode" name="carrierCode" value="@Model.SimpleAvailibilityaAddResponcelist[j].segments[i].identifier.carrierCode" />
                                                <input type="hidden" class="identifier" name="identifier" value="@Model.SimpleAvailibilityaAddResponcelist[j].segments[i].identifier.identifier" />
                                                <div class="fliCode"> @Model.SimpleAvailibilityaAddResponcelist[j].segments[i].identifier.carrierCode  - @Model.SimpleAvailibilityaAddResponcelist[j].segments[i].identifier.identifier </div>*@
                                                    <div class="fliCode"> @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.carrierCode  - @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.identifier </div>

                                                </div>
                                                <div class="fare-round">
                                                    @{
                                                        var departuretime = Model.SimpleAvailibilityaAddResponcelist[j].designator.departure;
                                                        string departuretimestr = departuretime.ToString();
                                                        var departuretimefinal = departuretimestr.Substring(11, 5);
                                                    }
                                                    <div class="">@departuretimefinal</div>
                                                    <div class="">@Model.SimpleAvailibilityaAddResponcelist[j].designator.origin</div>

                                                    <input type="hidden" class="departuretime" value="@departuretimefinal" />
                                                    <input type="hidden" class="origin" value="@Model.SimpleAvailibilityaAddResponcelist[j].designator.origin" />

                                                </div>
                                                <div class="fare-round">
                                                    @{
                                                        var arrivalDate = Model.SimpleAvailibilityaAddResponcelist[j].designator.arrival;
                                                        var departureDate = Model.SimpleAvailibilityaAddResponcelist[j].designator.departure;
                                                        var diff = (arrivalDate - departureDate);
                                                    }
                                                    <div class="col text-center">
                                                        <div class="px-20 md:px-0">@diff</div>
                                                        <div class="flightLine">
                                                            <div></div>
                                                            <div></div>
                                                        </div>
                                                        <div class="mt-1">@Model.SimpleAvailibilityaAddResponcelist[j].stops</div>
                                                        <input type="hidden" class="diff" value="@diff" />
                                                        <input type="hidden" class="stops" value="@Model.SimpleAvailibilityaAddResponcelist[j].stops" />
                                                    </div>
                                                </div>
                                                <div class="fare-round">
                                                    @{
                                                        var arrivaltime = Model.SimpleAvailibilityaAddResponcelist[j].designator.arrival;
                                                        string arrivaltimestr = arrivaltime.ToString();
                                                        var arrivalfinal = arrivaltimestr.Substring(11, 5);
                                                    }
                                                    <div class="lh-15 fw-500">@arrivalfinal</div>
                                                    <div class="text-15 lh-15 text-light-1">@Model.SimpleAvailibilityaAddResponcelist[j].designator.destination</div>

                                                    <input type="hidden" class="arrival" value="@arrivalfinal" />
                                                    <input type="hidden" class="destination" value="@Model.SimpleAvailibilityaAddResponcelist[j].designator.destination" />
                                                </div>

                                                <div class="fare-round mt-3">
                                                    <div class="rtprice">₹ @Model.SimpleAvailibilityaAddResponcelist[j].fareTotalsum</div>
                                                    <input type="hidden" class="fareTotalsum" value="@Model.SimpleAvailibilityaAddResponcelist[j].fareTotalsum" />

                                                </div>

                                                <div class="fare-round mt-3">
                                                    <input type="radio" id="uniqueId" name="dynamicRadio" value="@Model.SimpleAvailibilityaAddResponcelist[j].uniqueId" onclick="RenderData()">
                                                    @* <input class="farebtniddd" type="radio" name="radiosL" onclick="RenderData(@j)">*@
                                                </div>
                                            </div>

                                            <div class="rtflight-dtl showBtn"><a href="#">Flight Details <i class="fa fa-angle-down" data-unicode="f107"></i></a></div>
                                            <div class="hideme" style="display:none;">
                                                <div class="tabs -underline-2 pt-30 lg:pt-30 sm:pt-30 js-tabs">
                                                    <div class="tabs__controls row x-gap-2 y-gap-0 lg:x-gap-20 js-tabs-controls">

                                                        <div class="col-auto">
                                                            <button type="button" class="tabs__button text-light-1 fw-500 px-2 pb-2 lg:pb-0 js-tabs-button is-tab-el-active" data-tab-target=".-tab-item-1">Itinerary</button>
                                                        </div>

                                                        <div class="col-auto">
                                                            <button type="button" class="tabs__button text-light-1 fw-500 px-2 pb-2 lg:pb-0 js-tabs-button" data-tab-target=".-tab-item-2">Fare Details</button>
                                                        </div>

                                                        <div class="col-auto">
                                                            <button type="button" class="tabs__button text-light-1 fw-500 px-2 pb-2 lg:pb-0 js-tabs-button" data-tab-target=".-tab-item-3">Baggage</button>
                                                        </div>
                                                        <div class="col-auto">
                                                            <button type="button" class="tabs__button text-light-1 fw-500 px-2 pb-2 lg:pb-0 js-tabs-button" data-tab-target=".-tab-item-4">Cancellation</button>
                                                        </div>
                                                        <div class="col-auto">
                                                            <button type="button" class="tabs__button text-light-1 fw-500 px-2 pb-2 lg:pb-0 js-tabs-button" data-tab-target=".-tab-item-5">Fare Rule</button>
                                                        </div>

                                                    </div>

                                                    <div class="tabs__content pt-20 js-tabs-content">

                                                        <div class="tabs__pane -tab-item-1 is-tab-el-active">
                                                            <div class="col">
                                                                <div class="row y-gap-10 items-center">
                                                                    <div class="dst-top">@Model.SimpleAvailibilityaAddResponcelist[j].designator.origin<i class="fa fa-angle-double-right" aria-hidden="true"></i>@Model.SimpleAvailibilityaAddResponcelist[j].designator.destination</div>
                                                                    <div class="d-flex mt-2 justify-content-between">
                                                                        <div class="rtflight-hide">
                                                                            <img class="size-40" src="~/img/flight-logo-icon/1.webp" alt="image">
                                                                            <div class="lh-15 fw-500">AirAsia</div>
                                                                            @*  <div class="fliCode">@Model.SimpleAvailibilityaAddResponcelist[j].segments[i].identifier.carrierCode - @Model.SimpleAvailibilityaAddResponcelist[j].segments[i].identifier.identifier </div> *@
                                                                        </div>
                                                                        <div class="rtflight-hide">
                                                                            <div class="">@departuretimefinal</div>
                                                                            <div class="">@Model.SimpleAvailibilityaAddResponcelist[j].designator.origin</div>
                                                                            <span>Terminal-@Model.SimpleAvailibilityaAddResponcelist[j].departureTerminal </span>
                                                                        </div>
                                                                        <div class="rtflight-hide">
                                                                            <div class="px-20 md:px-0">@diff</div>
                                                                            <div class="flightLine">
                                                                                <div></div>
                                                                                <div></div>
                                                                            </div>
                                                                            <div class="mt-1">&nbsp;</div>
                                                                        </div>
