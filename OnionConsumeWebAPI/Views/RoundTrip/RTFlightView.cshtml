        document.addEventListener('DOMContentLoaded', function () {
            // Select the input fields for adults, children, and infants
            const adultInputField = document.getElementById('field_adult');
            const childInputField = document.getElementById('field_child');
            const infantInputField = document.getElementById('field_infant');

            // Select the elements to update the selected values
            const adultCountElement = document.getElementById('count_adult');
            const childCountElement = document.getElementById('count_child');
            const infantCountElement = document.getElementById('count_infant');

            // Add event listener for adult increment button
            document.querySelector('.increment.add.btn_sub').addEventListener('click', function () {
                incrementValue(adultInputField, adultCountElement);
            });

            // Add event listener for adult decrement button
            document.querySelector('.decrement.sub.btn_sub').addEventListener('click', function () {
                decrementValue(adultInputField, adultCountElement);
            });

            // Add event listener for child increment button
            document.querySelector('.increment1.add.btn_sub').addEventListener('click', function () {
                incrementValue(childInputField, childCountElement);
            });

            // Add event listener for child decrement button
            document.querySelector('.decrement1.sub.btn_sub').addEventListener('click', function () {
                decrementValue(childInputField, childCountElement);
            });

            // Add event listener for infant increment button
            document.querySelector('.increment2.add.btn_sub').addEventListener('click', function () {
                incrementValue(infantInputField, infantCountElement);
            });

            // Add event listener for infant decrement button
            document.querySelector('.decrement2.sub.btn_sub').addEventListener('click', function () {
                decrementValue(infantInputField, infantCountElement);
            });

            // Function to increment the input value and update the count element
            function incrementValue(inputField, countElement) {
                const currentValue = parseInt(inputField.value);
                if (currentValue < parseInt(inputField.max)) {
                    inputField.value = currentValue + 1;
                    countElement.textContent = currentValue + 1;
                }
            }

            // Function to decrement the input value and update the count element
            function decrementValue(inputField, countElement) {
                const currentValue = parseInt(inputField.value);
                if (currentValue > parseInt(inputField.min)) {
                    inputField.value = currentValue - 1;
                    countElement.textContent = currentValue - 1;
                }
            }
        });



