    <style type="text/css">
        .custom-flex p {
            font-size: 12px;
        }

        #myAnimation1 {
            width: 60px;
            height: 60px;
            position: absolute;
            bottom: 0px;
            border-radius: 50%;
            padding: 10px;
            background-color: red;
            display: none;
        }

        .rplc-btn {
            width: 34px;
            position: absolute;
            left: 177px;
            background: #ffffffc2;
            margin-top: 35px;
            margin-left: 20px;
            padding: 3px;
            border-radius: 50%;
            z-index: 999;
        }

            .rplc-btn img {
                transform: rotate(90deg);
            }

        .service-section .swiper-slide.active {
            border: none !important;
            background: #EBEDF8;
        }

        .btn-close {
            background-color: #fff !important;
            color: #000 !important;
            padding: 7px 9px;
            margin: 9px;
            border-radius: 8px;
        }

        .mbody-padding {
            padding: 8px 16px !important;
        }

        .fbody-padding {
            padding: 8px 16px !important;
        }

        .modal-mfareBlock .pd {
            padding: 8px 15px !important;
        }

        .model-wd80 {
            width: 90% !important;
            margin: auto;
        }

    </style>

</head>
<body>
    <main id="filternonstop">

        <!-- ***** Header Start ***** -->
        <header class="navbar navbar-sticky navbar-expand-lg inner-bg">
            <div class="container position-relative">
                <a class="navbar-brand" asp-controller="FlightSearchIndex" asp-action="Index">
