            //Sum Of Total fare left and right
            var totalL = localStorage.getItem('TotalfareL');
            var TotalfareLR = parseInt(totalL) + parseInt(fareTotalsumR, 10);
            $('#totalfaretotalround').text(TotalfareLR);

        });
        $('#ReturnButtonId').click(function () {
            debugger;
            $.ajax({
                type: 'POST',
                url: '/RoundTrip/PostReturnAATripsellView',
                data: {
                    uniqueId: uniqueId,
                    uniqueIdR: uniqueIdR
                },
                success: function (response) {

                    console.log('POST request successful');
                    $('#rtexampleModal .modal-body').html(response);
                    $('#rtexampleModal').modal('show');
                },
                error: function (xhr, status, error) {
                    console.error('Error occurred:', error);
                }
            });
        });
        $('.showBtn').click(function () {
            //$('.hideme').hide();
            if ($(this).hasClass('active')) {
                $(this).removeClass('active');
                $('.hideme').slideUp(1);
            } else {
                $('.hideme').slideUp(1);
                $('.showBtn').removeClass('active');
                $(this).addClass('active');
                $(this).next().filter('.hideme').slideDown(1);
            }
        });

    </script>
</body>
</html>


