<script>

    $(document).ready(function () {


        // //Start :MealKey post
     
            $('.select-option .servc-bordr').change((e) => {
                var selectedOptions = $('.select-option .servc-bordr option:selected');
                var mealCodes = [];
                var prices = [];
                //XBPJ_9000_RT1_Indigo
                selectedOptions.each(function () {
                    var optionValue = $(this).val();
                    var parts = optionValue.split('_');
                    mealCodes.push(optionValue);
                    prices.push(parseFloat(parts[1]));
                });
                //alert(mealCodes);
                localStorage.setItem("MealssrKeyCount", JSON.stringify(mealCodes));
                document.getElementById("hiddenSsrKey").value = mealCodes.join(',');

                // Calculate total price
                var totalPrice = prices.reduce((acc, curr) => acc + curr, 0);
                $('#AddMeal').text(totalPrice.toFixed(2));

                // Display meal codes and prices in separate alerts
                // alert("Meal Codes: " + mealCodes.join(','));
                //alert("Prices: " + prices.join(','));
                updateCombinedTotal();

            });


            $('.select-option2 .servc-bordr').change((e) => {
                debugger;
                var selected = $('.select-option2 .servc-bordr').map(function () {
                    return $(this).find("option:selected").map(function () {
                        return this.value;
                    }).get();
                }).get();


                var prices = $('.select-option2 .servc-bordr').map(function () {
                    return $(this).find("option:selected").map(function () {
                        var parts = this.value.split('_');
                        return parseFloat(parts[1]);
                    }).get();
                }).get();

                // Calculate the total price
                var totalPrice = prices.reduce((acc, curr) => acc + curr, 0);

                // Update the DOM elements with the selected values and total price
                //alert(selected);  // Display selected options for debugging
                localStorage.setItem("MealssrKeyCount2", JSON.stringify(selected));
                document.getElementById("hiddenSsrKey2").value = selected.join(','); // Joining selected values into a string

                // Display total price in a specified element, ensure you have an element with this ID
                $('#AddMeal2').text(totalPrice.toFixed(2));
                updateCombinedTotal();
            });


        function updateCombinedTotal() {
            var totalPrice1 = parseFloat($('#AddMeal').text()) || 0;
            var totalPrice2 = parseFloat($('#AddMeal2').text()) || 0;
            var combinedTotal = totalPrice1 + totalPrice2;
            $('#AddMealTotal').text(combinedTotal.toFixed(2));

        }
       





        //End :MealKey post

        //Bagage code start
  

            $('.select-baggage .servc-bordr').change((e) => {
                // Collect selected values and append "_0" to each
                var selected = $('.select-baggage .servc-bordr').map(function () {
                    return $(this).find("option:selected").map(function () {
                        return this.value + "_OneWay0";
                    }).get();
                }).get();

                // Extract prices from the selected values for total calculation
                var prices = $('.select-baggage .servc-bordr').map(function () {
                    return $(this).find("option:selected").map(function () {
                        var parts = this.value.split('_');
                        return parseFloat(parts[1]);
                    }).get();
                }).get();

                // Calculate the total price
                var totalPrice = prices.reduce((acc, curr) => acc + curr, 0);

                // Update the DOM elements with the selected values and total price
                //alert(selected);  // Debug: Display selected baggage options
                localStorage.setItem("BagssrKeyCount", JSON.stringify(selected));
                document.getElementById("hiddenbagKey").value = selected.join(',');  // Joining selected values into a string

                // Display total price in a specified element, assuming the ID is correct
                $('#AddBaggage').text(totalPrice.toFixed(2));
            });


      //Return Code start
      //Rt Meal


        $('.select-optionRT .servc-bordr').change((e) => {
            var selectedOptions = $('.select-optionRT .servc-bordr option:selected');
            debugger;

            var mealCodes = [];
            var prices = [];
            //XBPJ_9000_RT1_Indigo
            selectedOptions.each(function () {
                var optionValue = $(this).val();
                var parts = optionValue.split('_');

                mealCodes.push(optionValue);
                prices.push(parseFloat(parts[1]));
            });
            //alert(mealCodes);
            localStorage.setItem("MealssrKeyCountRT", JSON.stringify(mealCodes));
            document.getElementById("hiddenSsrKeyRT").value = mealCodes.join(',');

            // Calculate total price
            var totalPrice = prices.reduce((acc, curr) => acc + curr, 0);
            $('#AddMealRT').text(totalPrice.toFixed(2));

            // Display meal codes and prices in separate alerts
            // alert("Meal Codes: " + mealCodes.join(','));
            //alert("Prices: " + prices.join(','));
           

        });







        $('.select-option2RT .servc-bordr').change((e) => {
            debugger;
            var selected = $('.select-option2RT .servc-bordr').map(function () {
                return $(this).find("option:selected").map(function () {
                    return this.value;
                }).get();
            }).get();


            var prices = $('.select-option2RT .servc-bordr').map(function () {
                return $(this).find("option:selected").map(function () {
                    var parts = this.value.split('_');
                    return parseFloat(parts[1]);
                }).get();
            }).get();

            // Calculate the total price
            var totalPrice = prices.reduce((acc, curr) => acc + curr, 0);

            // Update the DOM elements with the selected values and total price
            //alert(selected);  // Display selected options for debugging
            localStorage.setItem("MealssrKeyCount2RT", JSON.stringify(selected));
            document.getElementById("hiddenSsrKey2RT").value = selected.join(','); // Joining selected values into a string

            // Display total price in a specified element, ensure you have an element with this ID
            $('#AddMeal2RT').text(totalPrice.toFixed(2));
           
        });


      
      // Rt Meal End 

      // Return Code End
        $('.select-baggageRT .servc-bordr').change((e) => {
            // Collect selected values and append "_0" to each
            var selected = $('.select-baggageRT .servc-bordr').map(function () {
                return $(this).find("option:selected").map(function () {
                    return this.value + "_RT0";
                }).get();
            }).get();

            // Extract prices from the selected values for total calculation
            var prices = $('.select-baggageRT .servc-bordr').map(function () {
                return $(this).find("option:selected").map(function () {
                    var parts = this.value.split('_');
                    return parseFloat(parts[1]);
                }).get();
            }).get();

            // Calculate the total price
            var totalPrice = prices.reduce((acc, curr) => acc + curr, 0);

            // Update the DOM elements with the selected values and total price
            //alert(selected);  // Debug: Display selected baggage options
            localStorage.setItem("BagssrKeyCountRT", JSON.stringify(selected));
            document.getElementById("hiddenbagKeyRT").value = selected.join(',');  // Joining selected values into a string

            // Display total price in a specified element, assuming the ID is correct
            $('#AddBaggageRT').text(totalPrice.toFixed(2));
        });

        // bagage code end

        $("#Continuebtn").click(function () {
            debugger;
            var unitKey = JSON.parse(localStorage.getItem("selectedIds"));
            // let mealcode1 = JSON.parse(localStorage.getItem("MealssrKeyCount"));
            // let mealcode2 = JSON.parse(localStorage.getItem("MealssrKeyCount2"));
            // var ssrKey = mealcode1 + "," + mealcode2;
            let mealcode1 = JSON.parse(localStorage.getItem("MealssrKeyCount"));
            let mealcode2 = JSON.parse(localStorage.getItem("MealssrKeyCount2"));
            let mealcode1RT = JSON.parse(localStorage.getItem("MealssrKeyCountRT"));
            let mealcode2RT = JSON.parse(localStorage.getItem("MealssrKeyCount2RT"));
             //var ssrKey;
            // if (mealcode2 && mealcode2.length > 0) {
            //     ssrKey = mealcode1.concat(mealcode2);
            // } else {
            //     ssrKey = mealcode1;
            // }

            // if (mealcode2RT && mealcode2RT.length > 0) {
            //     ssrKey = mealcode1RT.concat(mealcode2RT);
            // } else {
            //     ssrKey = mealcode1RT;
            // }
            var ssrKey = [];
            var BaggageSSrkey = [];
            if (mealcode2 && mealcode2.length > 0) {
                ssrKey = ssrKey.concat(mealcode1, mealcode2);
            } else {
                ssrKey = ssrKey.concat(mealcode1);
            }

            if (mealcode2RT && mealcode2RT.length > 0) {
                ssrKey = ssrKey.concat(mealcode1RT, mealcode2RT);
            } else {
                ssrKey = ssrKey.concat(mealcode1RT);
            }
            let BaggageSSrkey_0 = JSON.parse(localStorage.getItem("BagssrKeyCount"));
            let BaggageSSrkey_1 = JSON.parse(localStorage.getItem("BagssrKeyCountRT"));
            if (BaggageSSrkey_1 && BaggageSSrkey_1.length > 0) {
                BaggageSSrkey = BaggageSSrkey.concat(BaggageSSrkey_0, BaggageSSrkey_1);
            } else {
                BaggageSSrkey = BaggageSSrkey.concat(BaggageSSrkey_1);
            }
            $.ajax({
                url: "/RoundAATripsell/PostUnitkey",
                type: "POST",
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                data: { unitKey: unitKey, ssrKey: ssrKey, BaggageSSrkey: BaggageSSrkey },
                success: function (data) {
                    window.location.href = '/RoundTripPaymentGateway/RoundTripPaymentView';
                },
                error: function (err) {
                    console.error(err);
                }
            });
        });





        $("#exampleModalSeat").click(function (e) {
            debugger;
            e.preventDefault();
            $.ajax({
                url: '@Url.Action("RTPostSeatMapModaldataView", "RoundAATripsell")',
                type: "GET",
                success: function (data) {
                    //console.log('POST request successful');
                    $('#sgexampleModal .modal-bodyy').html(data);
                    $('#sgexampleModal').modal('show');

                },
                error: function (error) {
                    console.log("Error fetching data:", error);
                }
            });
        });

    });



</script>



