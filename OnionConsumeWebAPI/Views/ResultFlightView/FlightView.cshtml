                    <img class="navbar-brand-sticky" src="~/img/logo/logo.png" alt="sticky brand-logo">

                </a>
                <button class="navbar-toggler d-lg-none" type="button" data-toggle="navbarToggler" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="navbar-inner">
                    <!--  Mobile Menu Toggler -->
                    <button class="navbar-toggler d-lg-none" type="button" data-toggle="navbarToggler" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="nav-bar col-12 col-md-7 col-lg-12">
                        <ul class="nav nav-pills" id="pills-tab" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="pill" href="#"><img class="mr-2" src="~/img/nav-icon-1.png">Flight</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="pill"><img class="mr-2" src="~/img/nav-icon-2.png">Hotel</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="pill" href="#"><img class="mr-2" src="~/img/nav-icon-3.png">Visa</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="pill" href="#"><img class="mr-2" src="~/img/nav-icon-4.png">Holidays</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" data-toggle="pill" href="#"><img class="mr-2" src="~/img/nav-icon-5.png">Trains</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="pill" href="#"><img class="mr-2" src="~/img/nav-icon-6.png">Buses</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="pill"><img class="mr-2" src="~/img/nav-icon-7.png">Cabs</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" data-toggle="pill" href="#"><img class="mr-2" src="~/img/nav-icon-8.png">Flight Passes</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link"><img class="mr-2" src="~/img/nav-icon-8.png">Login</a>
                            </li>
                            <li>
                                <p class="nav-link" id="demo"></p>
                            </li>
                        </ul>
                    </div>
                    <nav id="mobile-nav">
                        <ul class="navbar-nav" id="navbar-nav">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#">
                                    Home
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link scroll" href="features">Features</a>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#">
                                    Pages
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link scroll" href="#">Screenshots</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link scroll" href="#">Pricing</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link scroll" href="#">Contact</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </header>
        <!-- ***** Header End ***** -->
        <!-- ***** Start inner search ***** -->
        <div class="inner-search">
            <!-- ***** Flight Search Area Start ***** -->
            <section class="section search-area bg-overlay  d-flex align-items-center">
                <div class="container">
                    <div class="row ">
                        <div class="col-12 col-md-7 col-lg-12">
                            <div class="row justify-content-center">
                                <div class="search-box">
                                    <div class="S-option">
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="Radio" value="option1" checked>
                                            <label class="form-check-label">One way</label>
                                        </div>
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="Radio" value="option2">
                                            <label class="form-check-label">Round Trip</label>
                                        </div>
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input" type="radio" name="Radio" value="option2">
                                            <label class="form-check-label">Multi City</label>
                                        </div>

                                    </div>

                                    <form asp-action="SearchResultFlight" asp-controller="FlightSearchIndex" method="post" id="multipleValuesForm">
                                        <div class="col-lg-12">
                                            <div class="row">
                                                <div class="col-12 col-md-10 col-lg-2 space autocomplete">
                                                    <label class=""></label>
                                                    <span class="input-group-text">
                                                        <input id="myInput" name="origin" type="text" value="@Model.simpleAvailabilityRequestModelEdit.origin" class="form-control" onclick="onewayDropdown()">
                                                        @* <span id="airportName"></span> *@
                                                    </span>
                                                    <span class="autodropdownOneWay">
                                                        <select id="autodropdownOneWayId" name="autodropdownOneWayId" onchange="OnewaySelection()">
                                                            @* <option value="">Please select</option> *@

                                                            @foreach (var item in Citynamelist.GetAllCityData())
                                                            {
                                                                <option value="@item.citycode">@item.cityname - @item.citycode -<br>@item.airportname </option>

                                                            }

                                                        </select>
                                                    </span>

                                                </div>

                                                <div class="rplc-btn">
                                                    <img src="~/img/direction-arrows.png" />
                                                </div>

                                                <div class="col-12 col-md-10 col-lg-2 space autocomplete">
                                                    <label class=""></label>
                                                    <span class="input-group-text" id="basic-addon1">
                                                        <input type="text" id="myInput1" name="destination" class="form-control" value="@Model.simpleAvailabilityRequestModelEdit.destination" onclick="toggleArrivalOneway()">
                                                        @* <span id="airportArval"></span> *@
                                                    </span>
                                                    <div id="errordestinaton" class="errorbox" style="display:none;">Enter Destination Name*</div>

                                                    <span class="autoarrivalOneway">
                                                        <select id="arrivalItemOnewayId" name="arrivalItemOnewayId" onchange="arrivalOneway()">
                                                            @* <option value="">Please select</option> *@

                                                            @foreach (var item in Citynamelist.GetAllCityData())
                                                            {
                                                                <option value="@item.citycode">@item.cityname - @item.citycode -<br>@item.airportname </option>

                                                            }

                                                        </select>
                                                    </span>

                                                </div>
                                                <div class="col-12 col-md-10 col-lg-2 space">
                                                    <label style="margin-bottom: 0.1rem;">Departure</label>
                                                    <span class="input-group-text">

                                                        <input id="Oneway-date" name="beginDate" type="text" value="@Model.simpleAvailabilityRequestModelEdit.beginDate" class="form-control">
                                                    </span>
                                                </div>
                                                <div class="col-12 col-md-10 col-lg-2 space">
                                                    <label style="margin-bottom: 0.1rem;">Return</label>
                                                    <span class="input-group-text" id="basic-addon1">

                                                        <input id="Oneway-endDate" name="endDate" type="text" value="@Model.simpleAvailabilityRequestModelEdit.endDate" class="form-control" disabled>
                                                    </span>
                                                </div>
                                                <div class="col-12 col-md-10 col-lg-2 space travellers">
                                                    <label style="margin-bottom: 0.1rem;">Travellers </label>
                                                    <div class="searchMenu-guests js-form-dd js-form-counters">
                                                        <div data-x-dd-click="searchMenu-guests">

                                                            <div class="text-15 text-light-1 ls-2 lh-16">
                                                                <span class="js-count-adult" id="count_adult">@Model.simpleAvailabilityRequestModelEdit.passengers.types[0].count</span> adults
                                                                @if (Model.simpleAvailabilityRequestModelEdit.passengers.types.Count > 1)
                                                                {
                                                                    <span class="js-count-child" id="count_child">@Model.simpleAvailabilityRequestModelEdit.passengers.types[1].count </span>

                                                                    <span>children</span>

                                                                }
                                                                @if (Model.simpleAvailabilityRequestModelEdit.passengers.types.Count > 2)
                                                                {
                                                                    <span class="js-count-infents" id="count_infant">@Model.simpleAvailabilityRequestModelEdit.passengers.types[2].count</span>

                                                                    <span>infant</span>
                                                                }
                                                            </div>
                                                        </div>


                                                        <div class="searchMenu-guests__field shadow-2" data-x-dd="searchMenu-guests" data-x-dd-toggle="-is-active">

                                                            <div class="bg-white px-30 py-30 rounded-4">

                                                                @*  ------------------------ *@
                                                                <div class="d-flex y-gap-10 justify-between items-center">
                                                                    <div class="btn-incr-adult">
                                                                        <div class="text-15 fw-500">Adults</div>

                                                                        <input type="hidden" name="adulttype" value="ADT" asp-for="@Model.simpleAvailabilityRequestModelEdit.passengercount.adulttype" />
                                                                        <div class="text-15 fw-500">Aged 12+ yrs</div>
                                                                    </div>
                                                                    <div class="btn-increase">
                                                                        <div class="decrement sub btn_sub">-</div>

                                                                        <input type="number" id="field_adult" class="input-field" name="adultcount" value="1" asp-for="@Model.simpleAvailabilityRequestModelEdit.passengercount.adultcount" min="1" max="9">
                                                                        <div class="increment add btn_sub">+</div>
                                                                    </div>
                                                                </div>
                                                                <div class="border-top-light mt-24 mb-24"></div>
                                                                <div class="d-flex y-gap-10 justify-between items-center">
                                                                    <div class="btn-incr-chld">
                                                                        <div class="text-15 fw-500">
                                                                            Children
                                                                        </div>
                                                                        <input type="hidden" name="childtype" value="CHD" asp-for="@Model.simpleAvailabilityRequestModelEdit.passengercount.childtype" />
                                                                        <div class="text-15 fw-500">
                                                                            Aged 2-12 yrs
                                                                        </div>
                                                                    </div>
                                                                    <div class="btn-increase">
                                                                        <div class="decrement1 sub btn_sub">-</div>
                                                                        <input type="number" id="field_child" class="input-field" value="0" name="childcount" asp-for="@Model.simpleAvailabilityRequestModelEdit.passengercount.childcount" min="0" max="6">
                                                                        <div class="increment1 add btn_sub">+</div>

                                                                    </div>
                                                                </div>
                                                                <div class="border-top-light mt-24 mb-24"></div>
                                                                <div class="d-flex y-gap-10 justify-between items-center">
                                                                    <div class="infant-area">
                                                                        <div class="text-15 fw-500">Infants</div>
                                                                        <input type="hidden" name="infanttype" value="INFT" asp-for="@Model.simpleAvailabilityRequestModelEdit.passengercount.infanttype" />
                                                                        <div class="text-15 fw-500">
                                                                            Below 2 yrs
                                                                        </div>
                                                                    </div>
                                                                    <div class="btn-increase">
                                                                        <div class="decrement2 sub btn_sub">-</div>

                                                                        <input type="number" id="field_infant" class="input-field" name="infantcount" value="0" asp-for="@Model.simpleAvailabilityRequestModelEdit.passengercount.infantcount" min="0" max="6">
                                                                        <div class="increment2 add btn_sub">+</div>

                                                                    </div>
                                                                </div>

                                                                <div class="border-top-light mt-24 mb-24"></div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-12 col-md-10 col-lg-2"><button style="margin-top:0px;" type="submit" class="btn btn-lg btn-block"><i class="fa fa-search mr-10" aria-hidden="true"></i>Modify Search</button></div>

                                                <div class="col-12 col-md-12 col-lg-12 search-bottom"></div>
                                                <div class="col-12 col-md-10 col-lg-9 pl-0">
                                                    <div class="form-group pt-2">
                                                        <div class="dropdown js-dropdown js-economy-active" style="display:inline-block;">
                                                            <div class="dropdown__button d-flex items-center  text-15" data-el-toggle=".js-economy-toggle" data-el-toggle-active=".js-economy-active">
                                                                <span class="js-dropdown-title">Show Class </span>
                                                                <i class="fa fa-angle-down ml-1 icon icon-chevron-sm-down" aria-hidden="true"></i>
                                                            </div>
                                                            <div class="toggle-element -dropdown  js-click-dropdown js-economy-toggle">
                                                                <div class="text-14 y-gap-8 js-dropdown-list">

                                                                    <div><a href="#" class="d-block js-dropdown-link">Economic</a></div>
                                                                    <div><a href="#" class="d-block js-dropdown-link">Premium</a></div>
                                                                    <div><a href="#" class="d-block js-dropdown-link">Business</a></div>
                                                                    <div><a href="#" class="d-block js-dropdown-link">First</a></div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="custom-control custom-checkbox custom-control-inline">
                                                            <input type="checkbox" class="custom-control-input" id="defaultInline3">
                                                            <label class="custom-control-label" for="defaultInline3">Direct Flights</label>
                                                        </div>
                                                        <div class="custom-control custom-checkbox custom-control-inline">
                                                            <input type="checkbox" class="custom-control-input" id="defaultInline4">
                                                            <label class="custom-control-label" for="defaultInline4">Defence Fare</label>
                                                        </div>
                                                        <div class="custom-control custom-checkbox custom-control-inline">
                                                            <input type="checkbox" class="custom-control-input" id="defaultInline5">
                                                            <label class="custom-control-label" for="defaultInline5">Student Fare </label>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </form>

                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </section>

            <!-- ***** Flight Search Area End ***** -->
        </div>


        <!-- ***** End inner search ***** -->
        <section class="layout-pt-md layout-pb-md bg-light-2 search-result">
            <div class="container">
                <div class="row y-gap-30">
                    <div class="col-xl-3 col-lg-4">
                        <aside class="sidebar py-20 px-20 rounded-4 bg-white">
                            <partial name="_FlightViewPartialView.cshtml" />
                        </aside>
                    </div>

                    <div class="col-xl-9 col-lg-8">
                        <div class="shorting-bg-white" style="padding: 0px 15px 15px 15px;">
                            <ul class="shorting-tab">
                                <li>
                                    <a asp-controller="ResultFlightView" asp-action="FlightView" asp-route-sortOrder="@ViewBag.NameSortParam">
                                        Airlines
                                        @if (ViewBag.NameSortParam == "name_desc")
                                        {
                                            <span class="oi oi-sort-descending"></span>
                                        }
                                        else
                                        {
                                            <span class="oi oi-sort-ascending"></span>
                                        }
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="ResultFlightView" asp-action="FlightView" asp-route-sortOrder="@ViewBag.DepartSortParam">
                                        Depart
                                        @if (ViewBag.PriceSortParam == "price_desc")
                                        {
                                            <span class="oi oi-sort-descending"></span>
                                        }
                                        else
                                        {
                                            <span class="oi oi-sort-ascending"></span>
                                        }
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="ResultFlightView" asp-action="FlightView" asp-route-sortOrder="@ViewBag.durationSortParam">
                                        Duration
                                        @if (ViewBag.PriceSortParam == "duration_desc")
                                        {
                                            <span class="oi oi-sort-descending"></span>
                                        }
                                        else
                                        {
                                            <span class="oi oi-sort-ascending"></span>
                                        }
                                    </a>
                                </li>
                                <li>
                                    <a asp-controller="ResultFlightView" asp-action="FlightView" asp-route-sortOrder="@ViewBag.arriveSortParam">
                                        Arrive
                                        @if (ViewBag.PriceSortParam == "arrive_desc")
                                        {
                                            <span class="oi oi-sort-descending"></span>
                                        }
                                        else
                                        {
                                            <span class="oi oi-sort-ascending"></span>
                                        }
                                    </a>
                                </li>
                                @*  <li><a href="#">Airlines </a></li> *@
                                @*  <li><a href="#">Depart</a></li>
                                <li><a href="#">Duration</a></li>
                                <li><a href="#">Arrive</a></li> *@
                                <li>
                                    <a asp-controller="ResultFlightView" asp-action="FlightView" asp-route-sortOrder="@ViewBag.PriceSortParam">
                                        Price
                                        @if (ViewBag.PriceSortParam == "price_desc")
                                        {
                                            <span class="oi oi-sort-descending"></span>
                                        }
                                        else
                                        {
                                            <span class="oi oi-sort-ascending"></span>
                                        }
                                    </a>
                                </li>
                            </ul>
                        </div>

                        @for (var j = 0; j < Model.SimpleAvailibilityaAddResponcelist.Count; j++)
                        {

                            //var collapseId = "ViewPriceCollapse" + Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.identifier.Trim();
                            var collapseId = "ViewPriceCollapse" + @j;
                            var CompanyName = "";
                            <div class="py-20 px-20 bg-white mt-20">
                                <div class="row y-gap-30 justify-between">
                                    <div class="col">
                                        <div class="row y-gap-10 items-center">

                                            <div class="col-sm-auto">
                                                @*   @{
                                            var imagePath = "/img/flight-logo-icon/" + TempData["AirAsiaLogin"];
                                            <td>
                                            <img class="size-40" src=@imagePath height="100px" width="100px" alt="image" />
                                            </td>
                                            } *@
                                                @if (Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.carrierCode.Equals("SG"))
                                                {

                                                    <img class="size-40" src="~/img/flight-logo-icon/SG.png" alt="image">
                                                }
                                                else if (Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.carrierCode.Equals("6E"))
                                                {

                                                    <img class="size-40" src="~/img/flight-logo-icon/6E.png" alt="image">
                                                }
                                                else
                                                {
                                                    <img class="size-40" src="~/img/flight-logo-icon/AIX_X_Logo_Orange.png" alt="image">
                                                }

                                            </div>

                                            <div class="col">
                                                <div class="row x-gap-20 items-end">
                                                    <div class="col-auto">

                                                        @{
                                                            @if (Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.carrierCode.Equals("SG"))
                                                            {
                                                                CompanyName = "SpiceJet";
                                                            }
                                                            else if (Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.carrierCode.Equals("6E"))
                                                            {

                                                                CompanyName = "Indigo";
                                                            }
                                                            else
                                                            {
                                                                CompanyName = "Air India Expr...";
                                                            }


                                                        }

                                                        <div class="lh-15 fw-500">@CompanyName</div>
                                                        @{
                                                            var SegmentDatacount = Model.SimpleAvailibilityaAddResponcelist[j].segments.Count;

                                                            if (SegmentDatacount == 1)
                                                            {
                                                                <div class="fliCode"> @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.carrierCode - @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].identifier.identifier </div>
                                                            }

                                                            if (SegmentDatacount == 2)
                                                            {
                                                                @for (var x = 0; x < SegmentDatacount; x++)
                                                                {
                                                                    if (x == 0)
                                                                    {

                                                                        <div class="fliCode"> @Model.SimpleAvailibilityaAddResponcelist[j].segments[x].identifier.carrierCode - @Model.SimpleAvailibilityaAddResponcelist[j].segments[x].identifier.identifier <b>,</b></div>

                                                                    }
                                                                    else
                                                                    {
                                                                        <div class="fliCode"> @Model.SimpleAvailibilityaAddResponcelist[j].segments[x].identifier.carrierCode - @Model.SimpleAvailibilityaAddResponcelist[j].segments[x].identifier.identifier</div>

                                                                    }

                                                                }
                                                            }
                                                        }
                                                    </div>

                                                    <div class="col-auto">

                                                        @{
                                                            var departuretime = @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].designator.departure;
                                                            string departuretimestr = departuretime.ToString();
                                                            var departuretimefinal = departuretimestr.Substring(11, 5);
                                                        }
                                                        <div class="">@departuretimefinal</div>
                                                        <div class="">@Model.SimpleAvailibilityaAddResponcelist[j].designator.origin</div>
                                                    </div>
                                                    @{
                                                        string formattedTime = string.Empty;
                                                        var arrivalDate = Model.SimpleAvailibilityaAddResponcelist[j].designator.arrival;
                                                        var departureDate = Model.SimpleAvailibilityaAddResponcelist[j].designator.departure;
                                                        var diff = (arrivalDate - departureDate);
                                                        TimeSpan timeSpan = TimeSpan.Parse(diff.ToString());
                                                        if ((int)timeSpan.Minutes == 0)
                                                            formattedTime = $"{(int)timeSpan.TotalHours} h";
                                                        else
                                                            formattedTime = $"{(int)timeSpan.TotalHours} h {(int)timeSpan.Minutes} m";

                                                    }
                                                    <div class="col text-center">
                                                        <div class="px-20 md:px-0">@formattedTime</div>
                                                        <div class="flightLine">
                                                            <div></div>
                                                            <div></div>
                                                        </div>
                                                        @{
                                                            var stops = Model.SimpleAvailibilityaAddResponcelist[j].stops;
                                                            if (stops == 0)
                                                            {
                                                                <div class="mt-1">Non Stop</div>
                                                            }
                                                            if (stops == 1)
                                                            {
                                                                <div class="mt-1">1 Stop via @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].legs[0].designator.destination </div>
                                                            }
                                                            if (stops == 2)
                                                            {
                                                                for (int l = 0; l < @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].legs.Count; l++)
                                                                {
                                                                    if (l == 0)
                                                                    {
                                                                        <div class="mt-1">2 Stop Via  @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].legs[l].designator.destination  <b>,</b></div>
                                                                    }
                                                                    else if (l == 1)
                                                                    {
                                                                        <span> @Model.SimpleAvailibilityaAddResponcelist[j].segments[0].legs[l].designator.destination</span>
                                                                    }
                                                                }
                                                            }
                                                        }
                                                        @*  <div class="mt-1">@flightResult.stops</div> *@
                                                    </div>

                                                    <div class="col-auto">

                                                        @{
                                                            var arrivaltime = Model.SimpleAvailibilityaAddResponcelist[j].designator.arrival;
                                                            string arrivaltimestr = arrivaltime.ToString();
                                                            var arrivalfinal = arrivaltimestr.Substring(11, 5);
